<?xml version="1.0"?>
<!DOCTYPE citydrain SYSTEM "model.dtd">

<citydrain version="1.0">

	<pluginpath path="./libnodes.so.1.0.0" />

	<simulation class="VarDTSimulation">
		<time start="0" stop="72000" dt="300" />
	</simulation>

	<model>
		<nodelist>
			<node id="rainread0" class="RainRead">
				<parameter name="file_name" type="string" value="models/Station_6620.ixx" />
			</node>
			
			<node id="fileout" class="FileOut">
				<parameter name="out_file_name" type="string" value="test-rainread.txt" />
			</node>
		</nodelist>
		
		<connectionlist>
			<connection id="con1">
				<source node="rainread0" port="out" />
				<sink	node="fileout" port="in" />
			</connection>
		</connectionlist>
	</model>
</citydrain>
